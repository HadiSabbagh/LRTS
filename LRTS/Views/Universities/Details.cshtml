@model Domain.Entities.University

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>University</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
       @*  <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LibraryId)
        </dd> *@
        
    </dl>
</div>
<div>
    <a id="delete-btn" asp-action="Delete" class="btn btn-danger" asp-route-id="@Model?.Id">Delete</a>
    <a id="edit-btn" asp-action="Edit" class="btn btn-info" asp-route-id="@Model?.Id">Edit</a>
    <a id="back-btn" href="##" class="btn btn-secondary" onClick="history.go(-1); return false;">Back</a>
</div>